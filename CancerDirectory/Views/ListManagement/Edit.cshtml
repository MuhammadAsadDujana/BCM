
@{
    ViewBag.Title = "Edit List";
}


<!-- Main content -->
<div class="content">
    <div class="container-fluid px-4">
        <div class="content-header">
            <h2 class="subHeading"></h2>
        </div>
        <div class="row">
            <div class="col-md-8">
                <!-- row -->
                <div class="profile-form">


                    <form id="myfrm">
                        <div class="row">
                            <div class="col-md-6">

                                @if (ViewBag.PageTitle != null)
                                {
                                    <div class="form-group">
                                        <h3 id="EditList">@ViewBag.PageTitle</h3>
                                    </div>
                                }


                                <div class="form-group">
                                    <label>Title</label>
                                    @if (ViewBag.GetSpecialties != null)
                                    {
                                        var item = (OA.Data.Model.Specialties)ViewBag.GetSpecialties;

                                        <input id="txtSpecialtyId" value="@item.SpecialtyId" type="hidden">
                                        <input required="" id="txtSpecialtyTitle" value="@item.SpecialtyTitle" type="text" class="form-control" name="">
                                    }
                                    else if (ViewBag.GetDiscipline != null)
                                    {
                                        var item = (OA.Data.Model.Discipline)ViewBag.GetDiscipline;

                                        <input id="txtDisciplineId" value="@item.DisciplineId" type="hidden">
                                        <input required="" id="txtDisciplineTitle" value="@item.DisciplineTitle" type="text" class="form-control" name="">
                                    }
                                    else if (ViewBag.GetTreatmentTeams != null)
                                    {
                                        var item = (OA.Data.Model.TreatmentTeam)ViewBag.GetTreatmentTeams;

                                        <input id="txtTreatmentTeamId" value="@item.TreatmentTeamId" type="hidden">
                                        <input required="" id="txtDiseaseTitle" value="@item.DiseaseTitle" type="text" class="form-control" name="">
                                    }
                                    else if (ViewBag.GetCredentials != null)
                                    {
                                        var item = (OA.Data.Model.Credentials)ViewBag.GetCredentials;

                                        <input id="txtCredentialsId" value="@item.CredentialsId" type="hidden">
                                        <input required="" id="txtCredentialsTitle" value="@item.CredentialsTitle" type="text" class="form-control" name="">
                                    }

                                </div>
                                <div class="buttons-group text-center mt-4">
                                    <a href="~/ListManagement/PhysicianListManagement" class="btn mr-2">Cancel</a>
                                    <button onclick="dataAccess.listManagementService.editListManagement()" type="button" class="btn flat-btn ml-2">Update</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content -->
