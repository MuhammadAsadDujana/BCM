
@{
    ViewBag.Title = "Add List";
}

<!-- Content Wrapper. Contains page content -->

    <!-- Main content -->
    <div class="content">
        <div class="container-fluid px-4">
            <div class="row">
                <div class="col-md-8">
                    <!-- row -->
                    <div class="profile-form">
                      

                        <form id="myfrm">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <h3>Add Specialty</h3>

                                    </div>

                                    <div class="form-group">
                                        <label>Title</label>
                                        <input required="" id="txtSpecialty" type="text" class="form-control" name="">
                                    </div>
                                    <div class="buttons-group text-center mt-4">
                                        <a href="~/ListManagement/PhysicianListManagement" class="btn mr-2">Cancel</a>
                                        <button onclick="dataAccess.listManagementService.addSpecialty()" type="button" class="btn flat-btn ml-2">Save</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content -->

<!-- /.content-wrapper -->
